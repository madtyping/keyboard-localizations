{
  "name": "@madtyping/keyboard-localizations",
  "version": "0.1.4",
  "description": "Keyboard layout definitions and types for physical and logical keyboard layouts",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "keyboard",
    "layout",
    "localization",
    "typing",
    "physical-layout",
    "logical-layout",
    "international"
  ],
  "author": "madtyping",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/madtyping/keyboard-localizations.git"
  },
  "bugs": {
    "url": "https://github.com/madtyping/keyboard-localizations/issues"
  },
  "homepage": "https://github.com/madtyping/keyboard-localizations#readme",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.1"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
